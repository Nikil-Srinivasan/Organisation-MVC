<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Organization</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Organization.styles.css" asp-append-version="true" />

    <link rel="shortcut icon" href="wwwroot/images/favicon.png" type="image/x-icon">
    <link rel="icon" href="wwwroot/images/favicon.ico" type="image/x-icon">

    <!-- Google font-->
    <link href="https://fonts.googleapis.com/css?family=Ubuntu:400,500,700" rel="stylesheet">

    <!-- themify -->
    <link rel="stylesheet" type="text/css" href="~/icon/themify-icons/themify-icons.css">

    <!-- iconfont -->
    <link rel="stylesheet" type="text/css" href="~/icon/icofont/css/icofont.css">

    <!-- simple line icon -->
    <link rel="stylesheet" type="text/css" href="~/icon/simple-line-icons/css/simple-line-icons.css">


    <!-- Chartlist chart css -->
    <link rel="stylesheet" href="~/lib/chartist/dist/chartist.css" type="text/css" media="all">

    <!-- Weather css -->
    <link href="~/css/svg-weather.css" rel="stylesheet">


    <!-- Style.css -->
    <link rel="stylesheet" type="text/css" href="~/css/main.css" asp-append-version="true" >

    <!-- Responsive.css--> 
    <link rel="stylesheet" type="text/css" href="~/css/responsive.css" asp-append-version="true">
</head>
<body>

    <div>
            @RenderBody()
     
    </div>

  
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="~/lib/Jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/lib/tether/dist/js/tether.min.js"></script>

    <!-- Scrollbar JS-->
    <script src="~/lib/jquery-slimscroll/jquery.slimscroll.js"></script>
    <script src="~/lib/jquery.nicescroll/jquery.nicescroll.min.js"></script>

    <!--classic JS-->
    <script src="~/lib/classie/classie.js"></script>

    <!-- notification -->
    <script src="~/lib/notification/js/bootstrap-growl.min.js"></script>

    <!-- Sparkline charts -->
    <script src="~/lib/jquery-sparkline/dist/jquery.sparkline.js"></script>

    <!-- Counter js  -->
    <script src="~/lib/waypoints/jquery.waypoints.min.js"></script>
    <script src="~/lib/countdown/js/jquery.counterup.js"></script>

    <!-- Echart js -->
    <script src="~/lib/charts/echarts/js/echarts-all.js"></script>

    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script src="https://code.highcharts.com/modules/exporting.js"></script>
    <script src="https://code.highcharts.com/highcharts-3d.js"></script>

    <!-- custom js -->
    <script src="~/js/main.min.js"></script>
    <script src="~/js/pages/dashboard.js"></script>
    <script src="~/js/pages/elements.js"></script>
    <script src="~/js/menu.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
